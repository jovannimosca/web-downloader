   "dependencies": {
      "@emotion/react": "^11.14.0",
      "@emotion/styled": "^11.14.0",
      "@fontsource/roboto": "^5.2.5",
      "@mui/icons-material": "^7.1.0",
      "@mui/material": "^7.1.0",
      "@mui/x-data-grid": "^7.24.0",
      "@tanstack/react-query": "^5.64.1",
      "axios": "^1.7.9",
      "dayjs": "^1.11.13",
      "dotenv": "^16.4.7",
      "install": "^0.13.0",
      "npm": "^11.4.1",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-hook-form": "^7.54.2"
   },
   "devDependencies": {
      "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
