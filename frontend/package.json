      "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.1",
    "msw": "^2.10.2",
    "prettier": "^3.5.3",
      "typescript": "~5.8.3",
      "typescript-eslint": "^8.18.2",
      "vite": "^6.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
